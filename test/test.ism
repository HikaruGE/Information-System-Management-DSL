entity Student{
	name : String
	phone : String
	subjects -> Set<Subject>
}

entity Subject{
	name : String
	enrollStu -> Set<Student>(dominatedBy="Student.subjects")
}

derive CRUD Student
derive CRUD Subject

init{

	sub1 := Subject{
				name := "Math"
				}
	Subject.save(sub1)
	
	
	sub2 := Subject{
				name := "English"
				}
	Subject.save(sub2)

	stu1 := Student{ 
				name := "s1"
				phone := "111"
				subjects := [sub1,sub2]
				}
	Student.save(stu1)
} 

template navbar(){
		list{
			listitem{gotoAll(Subject)}
			listitem{gotoAll(Student)}
		}
}

dispatcher main {

	/index >>
		page main(){
//			data{}
			body{
				template.navbar()
			}
		}
	
}

